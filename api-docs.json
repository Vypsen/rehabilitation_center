{
    "openapi": "3.0.2",
    "info": {
        "title": "Laravel",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http:\/\/localhost"
        }
    ],
    "paths": {
        "\/api\/user": {
            "get": {
                "tags": [
                    "auth"
                ],
                "summary": "isUser",
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application\/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "$ref": "#\/components\/schemas\/User"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                }
            }
        },
        "\/api\/auth\/register": {
            "post": {
                "tags": [
                    "auth"
                ],
                "summary": "register user",
                "parameters": [
                    {
                        "name": "name",
                        "in": "query",
                        "description": "user name",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "example": "user"
                    },
                    {
                        "name": "email",
                        "in": "query",
                        "description": "email user",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "example": "user@gmail.com"
                    },
                    {
                        "name": "password",
                        "in": "query",
                        "description": "password user",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response"
                    },
                    "422": {
                        "$ref": "#\/components\/responses\/ErrorValidation"
                    }
                }
            }
        },
        "\/api\/auth\/login": {
            "post": {
                "tags": [
                    "auth"
                ],
                "summary": "login user",
                "parameters": [
                    {
                        "name": "email",
                        "in": "query",
                        "description": "email user",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "example": "user@gmail.com"
                    },
                    {
                        "name": "password",
                        "in": "query",
                        "description": "password user",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "$ref": "#\/components\/responses\/UserToken"
                    },
                    "422": {
                        "$ref": "#\/components\/responses\/ErrorValidation"
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                }
            }
        },
        "\/api\/auth\/logout": {
            "post": {
                "tags": [
                    "auth"
                ],
                "summary": "logout user",
                "responses": {
                    "200": {
                        "description": "Successful logout"
                    }
                }
            }
        },
        "\/api\/blog": {
            "get": {
                "tags": [
                    "blog"
                ],
                "summary": "Display a listing of the resource.",
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application\/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#\/components\/schemas\/Post"
                                            }
                                        },
                                        "links": {
                                            "$ref": "#\/components\/schemas\/PaginatorLinks"
                                        },
                                        "meta": {
                                            "$ref": "#\/components\/schemas\/PaginatorMeta"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Not found"
                    }
                }
            }
        },
        "\/api\/blog\/{blog}": {
            "get": {
                "tags": [
                    "blog"
                ],
                "summary": "Show the specified resource.",
                "parameters": [
                    {
                        "name": "blog",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application\/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "$ref": "#\/components\/schemas\/Post"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Not found"
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "User": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    },
                    "email": {
                        "type": "string"
                    },
                    "password": {
                        "type": "string"
                    }
                }
            },
            "PaginatorLinks": {
                "type": "object",
                "properties": {
                    "first": {
                        "type": "string"
                    },
                    "last": {
                        "type": "string"
                    },
                    "prev": {
                        "type": "string"
                    },
                    "next": {
                        "type": "string"
                    }
                }
            },
            "PaginatorMeta": {
                "type": "object",
                "properties": {
                    "current_page": {
                        "type": "integer"
                    },
                    "from": {
                        "type": "integer"
                    },
                    "last_page": {
                        "type": "integer"
                    },
                    "links": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "url": {
                                    "type": "string"
                                },
                                "label": {
                                    "type": "string"
                                },
                                "active": {
                                    "type": "string"
                                }
                            }
                        }
                    },
                    "path": {
                        "type": "string"
                    },
                    "per_page": {
                        "type": "integer"
                    },
                    "to": {
                        "type": "integer"
                    },
                    "total": {
                        "type": "integer"
                    }
                }
            },
            "Post": {
                "type": "object",
                "properties": {
                    "title": {
                        "type": "string"
                    },
                    "slug": {
                        "type": "string"
                    },
                    "text": {
                        "type": "string"
                    },
                    "created_at": {
                        "type": "string"
                    }
                }
            }
        },
        "responses": {
            "ErrorValidation": {
                "description": "Validation errors",
                "content": {
                    "application\/json": {
                        "schema": {
                            "type": "array",
                            "example": {
                                "name": [
                                    "Something is wrong with this field!"
                                ]
                            }
                        }
                    }
                }
            },
            "UserToken": {
                "description": "User Token",
                "content": {
                    "application\/json": {
                        "schema": {
                            "type": "object",
                            "example": {
                                "token": "token_string"
                            }
                        }
                    }
                }
            }
        }
    },
    "tags": [
        {
            "name": "auth",
            "description": "auth"
        },
        {
            "name": "blog",
            "description": "blog"
        }
    ]
}
